{
  "Vendor": true,
  "DisableAll": true,
  "Enable": [
    "vet",
    "safesql",
    "errcheck",
    "goconst",
    "goimports",
    "varcheck",
    "staticcheck",
    "gosimple",
    "lll",
    "unconvert",
    "misspell",
    "unconvert"
  ],
  "Aggregate": true,
  "WarnUnmatchedNolint": true,
  "LineLength": 240,
  "Exclude": [
    "redundant return statement",
    "comment or be unexported",
    "comment on exported"
  ],
  "Deadline": "300s",
  "Skip": [ "cmd/sundial"]
}
